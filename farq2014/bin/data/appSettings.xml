            <INPUT name="Mask" type="IMAGE" path="images/mask_building.png"/>
            
        </INPUTS>

        <VISUAL_LAYERS>
            <VISUAL_LAYER name="GLITCH" type="GLITCH_1" inputSource="Cam"/>
            <VISUAL_LAYER name="Post" type="IMAGE_PROCESSOR" inputSource="Particle"/>
            
        </VISUAL_LAYERS>

        <MIXERS>
            <MIXER name="channel1" type="SIMPLE_BLEND">
                <INPUT_SOURCE name="Post"/>
                <!--INPUT_SOURCE name="Pic"/-->
                <INPUT_SOURCE name="vFlicker"/>
                <INPUT_SOURCE name="vStars"/>
            </MIXER>
            <MIXER name="channel2" type="SIMPLE_BLEND">
                <INPUT_SOURCE name="Particle"/>
