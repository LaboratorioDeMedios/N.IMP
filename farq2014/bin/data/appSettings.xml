<MAIN_SETTINGS>
    <SETTINGS>
        <INPUTS>
            <INPUT name="Camera Input2" type="CAM"/>
            
            <INPUT name="Pic" type="IMAGE" palindrom="true">
                <ASSET name="satellite" path="images/satellite"/>
                <ASSET name="stars" path="sets/space/Stars1-Visual.mov"/>
                <ASSET name="stars" path="sets/space/Stars2-Visual.mov"/>
                <ASSET name="test" path="sets/contorno ciudad.mov"/>
                <ASSET name="test2" path="sets/frontal edificios.mov"/>
            </INPUT>
            <INPUT name="Pic2" type="IMAGE" palindrom="true">
                <ASSET name="flicker" path="movies/flicker.mov"/>
                <ASSET name="stars" path="movies/stars.mov"/>
                <ASSET name="stars" path="sets/space/Stars1-Visual.mov"/>
                <ASSET name="stars" path="sets/space/Stars2-Visual.mov"/>
                
                
            </INPUT>
            <INPUT name="Particle" type="PARTICLE"/>
            
        </INPUTS>

        <VISUAL_LAYERS>
            <!-- General Pipeline  Visual_Layers-->
            <VISUAL_LAYER name="General Visual 0" type="IMAGE_PROCESSOR" inputSource="General Input Selector 0"/>
            <VISUAL_LAYER name="General Visual 1" type="IMAGE_PROCESSOR" inputSource="General Input Selector 1"/>
            <!-- General Pipeline  Visual_Layers END-->

            <!-- Particle Mask Pipeline  Visual_Layers-->
            <VISUAL_LAYER name="Particle mix 0" type="IMAGE_PROCESSOR" inputSource="Particle"/>
            <VISUAL_LAYER name="Particle mix 1" type="IMAGE_PROCESSOR" inputSource="Pic"/>
            <!-- Particle Mask Pipeline  Visual_Layers END-->

            <!-- Movie Mix Pipeline  Visual_Layers-->
            <VISUAL_LAYER name="Movie mix 0" type="IMAGE_PROCESSOR" inputSource="Pic2"/>
            <VISUAL_LAYER name="Movie mix 1" type="IMAGE_PROCESSOR" inputSource="Pic"/>
            <!-- Movie Mix Pipeline  Visual_Layers END-->

            
        </VISUAL_LAYERS>

        <MIXERS>
            <!-- General Pipeline Mixers-->
            <MIXER name="General mix" type="SIMPLE_BLEND" selectorRight="1">
                <INPUT_SOURCE name="General Visual 0"/>
                <INPUT_SOURCE name="General Visual 1"/>
            </MIXER>
            <MIXER name="General Input Selector 0" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Pic"/>
                <INPUT_SOURCE name="Particle"/>
            </MIXER>
            <MIXER name="General Input Selector 1" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Pic"/>
                <INPUT_SOURCE name="Particle"/>
            </MIXER>
            <!-- General Pipeline Mixers END-->

            <!-- Particle Mask Pipeline Mixers-->
            <MIXER name="Particle mix" type="SIMPLE_BLEND" selectorRight="1">
                <INPUT_SOURCE name="Particle mix 0"/>
                <INPUT_SOURCE name="Particle mix 1"/>
            </MIXER>
            <!-- Particle Mask Pipeline Mixers END-->

            <!-- Movie Mix Pipeline Mixers-->
            <MIXER name="Movie mix" type="SIMPLE_BLEND" selectorRight="1">
                <INPUT_SOURCE name="Movie mix 0"/>
                <INPUT_SOURCE name="Movie mix 1"/>
            </MIXER>
            <!-- Movie Mix Pipeline Mixers END-->

            <!-- MIX for Columns -->
            <MIXER name="Columns" type="SIMPLE_BLEND" selectorRight="1">
                <INPUT_SOURCE name="selColumns1"/>
                <INPUT_SOURCE name="selColumns2"/>
            </MIXER>
            <!-- add input channels to all of the mixers -->
            <MIXER name="selColumns1" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Movie mix"/>
                <INPUT_SOURCE name="General mix"/>
                <INPUT_SOURCE name="Particle mix"/>
            </MIXER>
            <MIXER name="selColumns2" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Movie mix"/>
                <INPUT_SOURCE name="General mix"/>
                <INPUT_SOURCE name="Particle mix"/>
            </MIXER>
            
            <!-- MIX for Background -->
            <MIXER name="Background" type="SIMPLE_BLEND" selectorRight="1">
                <INPUT_SOURCE name="selBackground1"/>
                <INPUT_SOURCE name="selBackground2"/>
            </MIXER>
            <!-- add input channels to all of the mixers -->
            <MIXER name="selBackground1" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="General mix"/>
                <INPUT_SOURCE name="Particle mix"/>
                <INPUT_SOURCE name="Movie mix"/>
            </MIXER>
            <MIXER name="selBackground2" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="General mix"/>
                <INPUT_SOURCE name="Particle mix"/>
                <INPUT_SOURCE name="Movie mix"/>
            </MIXER>

            

        </MIXERS>

    </SETTINGS>

    <NODE_VIEWS>
        <!-- Final Mix Node View -->
        <NODE_VIEW name="FinalMix">
            <!-- columns -->
            <NODE name="Columns" x="675" y="20" guiX="675" guiY="240" imageScale="0.4" guiWidth="200" />
            <NODE name="selColumns1" x="20" y="20" guiX="450" guiY="40" imageScale="1.0" guiWidth="200" />
            <NODE name="selColumns2" x="940" y="20" guiX="1300" guiY="40" imageScale="1.0" guiWidth="200" />

            <!-- background -->
            <NODE name="Background" x="675" y="450" guiX="675" guiY="690" imageScale="0.4" guiWidth="200" />
            <NODE name="selBackground1" x="20" y="450" guiX="450" guiY="470" imageScale="1.0" guiWidth="200" />
            <NODE name="selBackground2" x="940" y="450" guiX="1300" guiY="470" imageScale="1.0" guiWidth="200" />
        </NODE_VIEW>
        <!-- Final Mix Node View END-->
        
        <!-- Particle Mask Pipeline Node View -->
        <NODE_VIEW name="Particle Mix">
            <NODE name="Particle" x="20" y="20" guiX="20" guiY="250" imageScale="0.5" guiWidth="200" />
            <NODE name="Particle mix 0" x="680" y="20" guiX="1010" guiY="20" imageScale="0.5" guiWidth="200" />
            <NODE name="Pic" x="20" y="650" guiX="240" guiY="250" imageScale="0.5" guiWidth="200" />
            <NODE name="Particle mix 1" x="620" y="500" guiX="450" guiY="20" imageScale="0.5" guiWidth="200" />
            <NODE name="Particle mix" x="1250" y="300" guiX="1250" guiY="550" imageScale="0.5" guiWidth="200" />
        </NODE_VIEW>
        <!-- Particle Mask Pipeline Node View END-->

        <!-- Movie Mix Pipeline Node View -->
        <NODE_VIEW name="Movie Mix">
            <NODE name="Pic" x="20" y="20" guiX="20" guiY="250" imageScale="0.5" guiWidth="200" />
            <NODE name="Movie mix 1" x="680" y="20" guiX="1010" guiY="20" imageScale="0.5" guiWidth="200" />
            <NODE name="Pic2" x="20" y="650" guiX="240" guiY="250" imageScale="0.5" guiWidth="200" />
            <NODE name="Movie mix 0" x="620" y="500" guiX="450" guiY="20" imageScale="0.5" guiWidth="200" />
            <NODE name="Movie mix" x="1250" y="300" guiX="1250" guiY="550" imageScale="0.5" guiWidth="200" />
        </NODE_VIEW>
        <!-- Movie Mix Pipeline Node View END-->
        
        <!-- General Pipeline Node View -->
        <NODE_VIEW name="General Mix">
            <NODE name="General Input Selector 0" x="20" y="20" guiX="20" guiY="400" imageScale="0.5" guiWidth="200" />
            <NODE name="General Visual 0" x="680" y="20" guiX="1010" guiY="20" imageScale="0.5" guiWidth="200" />
            <NODE name="General Input Selector 1" x="20" y="650" guiX="240" guiY="400" imageScale="0.5" guiWidth="200" />
            <NODE name="General Visual 1" x="620" y="500" guiX="450" guiY="20" imageScale="0.5" guiWidth="200" />
            <NODE name="General mix" x="1250" y="300" guiX="1250" guiY="550" imageScale="0.5" guiWidth="200" />
        </NODE_VIEW>
        <!-- General Pipeline Node View END-->

    </NODE_VIEWS>
    
    <PARAM_INPUT_GENERATORS>
        <INPUT_GEN name="KORG_NANO" type="MIDI" midiDeviceName="nanoKONTROL2 SLIDER/KNOB"/>
        <INPUT_GEN name="OSC" type="OSC" port="12345"/>
        <INPUT_GEN name="FFT1" type="FFT" />
    </PARAM_INPUT_GENERATORS>

    <SYPHON_SERVERS>
        <SERVER inputName="Background" exportName="bg"/>
        <SERVER inputName="Columns" exportName="Columns"/>
    </SYPHON_SERVERS>

</MAIN_SETTINGS>