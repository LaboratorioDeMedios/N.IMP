<MAIN_SETTINGS>
    <SETTINGS>
        <INPUTS>
            <!-- load a Single Image  -->
            <INPUT name="Mask" type="IMAGE" path="images/mask.png"
                   />
            <!-- load a Image Sequence -->
            <INPUT name="Pic" type="IMAGE" path="test"
                isPlaying="false" 
                   bpm="120" 
                   matchBPMtoSequence="false" 
                   multiplier_divider="2"
                   palindrom="true"
            />
            
        </INPUTS>

        <VISUAL_LAYERS>
            
        </VISUAL_LAYERS>

        <MIXERS>
            <MIXER name="mainMix" type="SIMPLE_BLEND">
                <INPUT_SOURCE name="Pic"/>
                <INPUT_SOURCE name="Mask"/>
            </MIXER>
        </MIXERS>

    </SETTINGS>

    <NODE_VIEWS>

        <NODE_VIEW name="OUTPUT">
            <NODE name="mainMix" x="20" y="20" guiX="1370" guiY="20" imageScale="1.35" guiWidth="200" />
            <NODE name="Pic" x="800" y="400" guiX="680" guiY="400" imageScale="0.5" guiWidth="200" />
        </NODE_VIEW>
        
    </NODE_VIEWS>
</MAIN_SETTINGS>