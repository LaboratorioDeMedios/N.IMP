    <SETTINGS>
        <INPUTS>
            <INPUT name="Cam" type="CAM"/>
            <INPUT name="vStars" type="VIDEO">
                <VIDEO path="movies/stars.mov" />
            </INPUT>
            <INPUT name="vFlicker" type="IMAGE">
                <ASSET name="stars" path="movies/stars.mov" />
                <ASSET name="mask" path="images/mask_building.png" />
            </INPUT>
            
            <INPUT name="vParticle" type="PARTICLE"/>
            
            <!--INPUT name="Pic" type="IMAGE" path="test"
